@import 'system'

.pageNotFound
  text-align center
  margin auto

  .Ku
    display inline-block

  img 
    padding 200px 0 0 0 
    width 280px
    height 280px

  .output
    width 240px
    margin auto

  a
    color default_color
    text-decoration none
    cursor pointer
    font-size 32px
    font-weight bold
    margin 5px
  a:hover
    color primary_color

  .ku-animation-enter-active
    animation ku-animation .5s

  .ku-animation-leave-active
    animation ku-animation .5s reverse

  @keyframes ku-animation
    0%
      transform scale(0)
    50%
      transform scale(1.5)
    100%
      transform scale(1)

